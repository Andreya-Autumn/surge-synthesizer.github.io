---
import Manual from "@layouts/manual.astro";
import Page from "@components/page.astro";

import { getEntry } from "astro:content";

const rack_xt_manual = await getEntry("pages", "rack_xt_manual");

const { Content } = await rack_xt_manual.render();

const title = rack_xt_manual.data.title;
---

<Manual title={title} {...Astro.props}>
    <Page title={title}>
        <Content />
    </Page>
</Manual>
